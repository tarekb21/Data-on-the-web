@startuml
Class Cafeteria {

name,
menu
}

Class Item {

name, 
price, 
kind

}

Class Coffe{

}

Class Tea{

}

Class Cake {

}


Class Ingredient{
sugar
}
Class Beans{

kind, 
level 
}

Cafeteria "1" -right->"1..*" Item: Provides
Coffe -up-|> Item
Tea -up-|> Item
Cake -up-|> Item
Coffe -down-> Beans
Cake -down-> Ingredient
@enduml
